{
  "name": "@prompt-matrix/core",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "dotenv": "^16.4.7",
    "tsup": "^8.0.2",
    "typescript": "^5.8.2",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "openai": "^4.83.0",
    "zod": "^3.22.4",
    "uuid": "^11.0.5"
  }
}

